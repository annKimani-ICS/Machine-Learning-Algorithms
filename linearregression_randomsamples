        "epochs = 1000\n",
        "\n",
        "#Arrays to store the values\n",
        "mse_array = []\n",
        "slope_array = []\n",
        "bias_array = []\n",
        "\n",
        "#Gradient descent loop\n",
        "for i in range(epochs):\n",
        "  y_pred = slope * x + bias\n",
        "  error = y_pred - y\n",
        "\n",
        "  #Calculating gradients\n",
        "  slope -= learning_rate * (2/len(x)) * np.sum(error * x)\n",
        "  bias -= learning_rate * (2/len(x)) * np.sum(error)\n",
        "\n",
        "  #Calculating MSE and store results\n",
        "  mse = np.mean(error ** 2)\n",
        "  mse_array.append(mse)\n",
        "  slope_array.append(slope)\n",
        "  bias_array.append(bias)\n",
        "\n",
        "  #Finding lowest MSE (optimal parameters)\n",
        "  min_mse_index = np.argmin(mse_array)\n",
        "  min_mse_slope = slope_array[min_mse_index]\n",
        "  min_mse_bias = bias_array[min_mse_index]\n",
        "\n",
        "  #Print results\n",
        "  print(f\"Optimal Slope: {min_mse_slope:.4f}\")\n",
        "  print(f\"Optimal Bias: {min_mse_bias:.4f}\")\n"
      ]
    }
  ]
}